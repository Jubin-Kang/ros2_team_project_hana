<?xml version='1.0' ?>

<launch>
    <arg name="use_sim_time" default="False"/>
    <arg name="motor_ratio" default="1.1"/>

    <include file="$(find-pkg-share hana_description)/launch/upload.launch.xml">
        <arg name="use_sim_time" value="$(var use_sim_time)"/>
    </include>

    <include file="$(find-pkg-share sllidar_ros2)/launch/sllidar_c1_launch.py">
    </include>

    <include file="$(find-pkg-share ros2_icm20948)/icm20948_node_launch.py">
    </include>

    <node pkg="hana_bringup" exec="hana_bringup">
        <param name="motor_ratio" value="$(var motor_ratio)"/>
    </node>

    <!-- <node pkg="hana_bringup" exec="camera">
    </node> -->
    <node pkg="hana_bringup" exec="hana_battery_publiser">
    </node>
    
</launch>
